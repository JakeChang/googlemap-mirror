# Google Maps Scraper 設定

# 每次搜尋最小間隔（毫秒）
SCRAPER_MIN_INTERVAL=60000

# 單次搜尋最大網格數（限制大範圍搜尋）
SCRAPER_MAX_GRIDS=16

# 單次搜尋最大結果數
SCRAPER_MAX_RESULTS=200

# 頁面載入後延遲（毫秒）- 最小值
SCRAPER_PAGE_DELAY_MIN=3000

# 頁面載入後延遲（毫秒）- 最大值
SCRAPER_PAGE_DELAY_MAX=5000

# 滾動間隔延遲（毫秒）- 最小值
SCRAPER_SCROLL_DELAY_MIN=2000

# 滾動間隔延遲（毫秒）- 最大值
SCRAPER_SCROLL_DELAY_MAX=3500

# 網格切換延遲（毫秒）- 最小值
SCRAPER_GRID_DELAY_MIN=3000

# 網格切換延遲（毫秒）- 最大值
SCRAPER_GRID_DELAY_MAX=5000

# 網格大小（度）- 約 0.08 度 ≈ 8km
SCRAPER_GRID_SIZE=0.08
